      "title": {
        "type": "Text",
        "config": {
          "label": "Titre du projet"
        }
      },
      "url": {
        "type": "Link",
        "config": {
          "allowTargetBlank": true,
          "label": "Url du projet",
          "select": null
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "Date de réalisation"
        }
      },
      "skills": {
        "type": "Group",
        "config": {
          "fields": {
            "skill": {
              "type": "Text",
              "config": {
                "label": "Une compétence"
              }
            }
          },
          "label": "Compétences mise en oeuvre"
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "single": "paragraph",
          "label": "Description du projet"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "media": {
              "type": "SharedSlice"
            }
          }
        }
      }
