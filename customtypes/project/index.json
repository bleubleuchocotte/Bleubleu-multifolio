{
  "id": "project",
  "label": "Project",
  "format": "custom",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Titre du projet",
          "placeholder": ""
        }
      },
      "project": {
        "type": "Link",
        "config": {
          "label": "Url du projet",
          "placeholder": "",
          "allowTargetBlank": true,
          "select": null
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "Date de réalisation",
          "placeholder": ""
        }
      },
      "skills": {
        "type": "Group",
        "config": {
          "label": "Compétences mise en oeuvres",
          "fields": {
            "name": {
              "type": "Text",
              "config": {
                "label": "Une compétence",
                "placeholder": ""
              }
            }
          }
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Description du projet",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      }
    },
    "Desktop": {
      "medias": {
        "type": "Group",
        "config": {
          "label": "Médias",
          "fields": {
            "principal": {
              "type": "Link",
              "config": {
                "label": "Média principal",
                "placeholder": "",
                "select": "media"
              }
            },
            "secondary": {
              "type": "Link",
              "config": {
                "label": "Média secondaire",
                "placeholder": "",
                "select": "media"
              }
            }
          }
        }
      }
    },
    "Mobile": {
      "media_mobile": {
        "type": "Link",
        "config": {
          "label": "Média | Mobile",
          "placeholder": "",
          "select": null
        }
      }
    }
  }
}
